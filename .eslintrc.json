{
  "extends": [
    "@papermana/eslint-config-personal",
    "@papermana/eslint-config-personal/react"
  ],
  "env": {
    "browser": true,
    "commonjs": true,
    "jest": true,
    "jasmine": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "plugins": [
    "jasmine"
  ],
  "rules": {
    "func-style": [1, "expression", {"allowArrowFunctions": true}]
  }
}
